<?xml version="1.0" encoding="utf-8" ?>
<Defs>

    <!--培养水晶-->
    <RecipeDef>
      <defName>TNA_CultivateCrystal</defName>
	    <label>cultivate crystal</label>
      <description>Cultivate crystal.</description>
      <jobString>Cultivating crystal.</jobString>
      <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
      <workAmount>30000</workAmount>
      <workSkill>Crafting</workSkill>
      <soundWorking>Recipe_Machining</soundWorking>
      <effectWorking>Smith</effectWorking>
      <researchPrerequisite>TNA_Crystalology</researchPrerequisite>
      <skillRequirements>
       <Crafting>15</Crafting>
      </skillRequirements>
      <recipeUsers>
		   <li>TNA_crafttable</li>
	    </recipeUsers>
      <unfinishedThingDef>UnfinishedHealthItemProsthetic</unfinishedThingDef>
      <allowMixingIngredients>true</allowMixingIngredients>
      <ingredients>
        <li>
          <filter>
            <thingDefs>
              <li>Gold</li>
            </thingDefs>
          </filter>
          <count>0.5</count>
        </li>
        <li>
          <filter>
            <thingDefs>
              <li>Uranium</li>
            </thingDefs>
          </filter>
          <count>5</count>
        </li>
        <li>
          <filter>
            <thingDefs>
              <li>TNA_crystal</li>
            </thingDefs>
          </filter>
          <count>20</count>
        </li>
      </ingredients>
      <products>
        <TNA_crystal>40</TNA_crystal>
      </products>
    </RecipeDef>

