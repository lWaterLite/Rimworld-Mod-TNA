<?xml version="1.0" encoding="utf-8"?>
<Defs>

    <ThingDef Abstract="True" Name="TNA_BaseWeaponRanged">
        <thingClass>ThingWithComps</thingClass>
        <category>Item</category>
        <drawGUIOverlay>true</drawGUIOverlay>
        <altitudeLayer>Item</altitudeLayer>
        <alwaysHaulable>true</alwaysHaulable>
        <tickerType>Never</tickerType>
        <equipmentType>Primary</equipmentType>
        <useHitPoints>true</useHitPoints>
        <pathCost>10</pathCost>
        <selectable>true</selectable>
        <statBases>
            <MaxHitPoints>500</MaxHitPoints>
            <Flammability>0</Flammability>
            <DeteriorationRate>0</DeteriorationRate>
            <Beauty>5</Beauty>
            <SellPriceFactor>1.20</SellPriceFactor>
            <WorkToMake>240000</WorkToMake>
        </statBases>
        <comps>
            <li Class="CompProperties_Forbiddable"/>      
            <li>
                <compClass>CompEquippable</compClass>
            </li>
            <li>
              <compClass>CompQuality</compClass>
            </li>
        </comps>
        <graphicData>
            <onGroundRandomRotateAngle>35</onGroundRandomRotateAngle>
        </graphicData>
        <techLevel>Archotech</techLevel>
        <smeltable>false</smeltable>
        <burnableByRecipe>false</burnableByRecipe>
        <thingCategories>
            <li>WeaponsRanged</li>
        </thingCategories>
        <inspectorTabs>
            <li>ITab_Art</li>
        </inspectorTabs>
        <weaponTags>
            <li>ArchotechGun</li>
        </weaponTags>
        <recipeMaker>
            <researchPrerequisite>TNA_RCS_Res</researchPrerequisite>
            <skillRequirements>
                <Crafting>20</Crafting>
            </skillRequirements>
            <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
            <workSkill>Crafting</workSkill>
            <effectWorking>Smith</effectWorking>
            <soundWorking>Recipe_Machining</soundWorking>
            <recipeUsers>
                <li>TNA_crafttable</li>
            </recipeUsers>
            <unfinishedThingDef>UnfinishedWeapon</unfinishedThingDef>
        </recipeMaker>
    </ThingDef>

    <!--ET-X55-->
    <ThingDef ParentName="BaseBullet">
    <defName>TNA_Bullet_ETX55LMG</defName>
    <label>charge shot</label>
    <graphicData>
      <texPath>Things/Projectile/Charge_Small</texPath>
      <graphicClass>Graphic_Single</graphicClass>
	  <shaderType>TransparentPostLight</shaderType>
    </graphicData>
    <projectile>
      <damageDef>Bullet</damageDef>
      <damageAmountBase>10</damageAmountBase>
      <armorPenetrationBase>0.3</armorPenetrationBase>
      <speed>70</speed>
    </projectile>
    </ThingDef>

    <ThingDef ParentName="TNA_BaseWeaponRanged">
        <defName>TNA_ETX55LMG</defName>
        <label>ET-X55</label>
        <description>ET-X55,standard LMG in Elements,specializable.</description>
        <graphicData>
            <texPath>Things/Weapon/Ranged/TNA_ETX55</texPath>
            <graphicClass>Graphic_Single</graphicClass>
        </graphicData>
        <soundInteract>TNA_Interact_ETX55</soundInteract>
        <statBases>
            <Mass>2.5</Mass>
            <AccuracyTouch>0.60</AccuracyTouch>
            <AccuracyShort>0.50</AccuracyShort>
            <AccuracyMedium>0.90</AccuracyMedium>
            <AccuracyLong>0.80</AccuracyLong>
            <RangedWeapon_Cooldown>1</RangedWeapon_Cooldown>
        </statBases>
    <costList>
        <TNA_crystal>400</TNA_crystal>
        <ComponentSpacer>10</ComponentSpacer>
    </costList>
    <verbs>
      <li>
        <verbClass>Verb_Shoot</verbClass>
        <hasStandardCommand>true</hasStandardCommand>
        <defaultProjectile>TNA_Bullet_ETX55LMG</defaultProjectile>
        <warmupTime>1.5</warmupTime>
        <range>35</range>
        <ticksBetweenBurstShots>4</ticksBetweenBurstShots>
        <burstShotCount>15</burstShotCount>
        <soundCast>TNA_Shot_ETX55</soundCast>
        <soundCastTail>GunTail_Medium</soundCastTail>
        <muzzleFlashScale>9</muzzleFlashScale>
      </li>
    </verbs>
    <tools>
      <li>
        <label>stock</label>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>10</power>
        <cooldownTime>1</cooldownTime>
      </li>
      <li>
        <label>barrel</label>
        <capacities>
          <li>Blunt</li>
          <li>Poke</li>
        </capacities>
        <power>10</power>
        <cooldownTime>1</cooldownTime>
      </li>
    </tools>
  </ThingDef>

