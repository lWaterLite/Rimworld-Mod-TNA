<?xml version="1.0" encoding="utf-8" ?>
<Defs>

    <HediffDef Name="TNA_DiseaseBase" Abstract="True">
      <hediffClass>HediffWithComps</hediffClass>
      <defaultLabelColor>(0.2, 0.8, 0.66)</defaultLabelColor>
      <initialSeverity>1</initialSeverity>
      <maxSeverity>1</maxSeverity>
    </HediffDef>

    <!--水晶血-->
    <HediffDef ParentName="TNA_DiseaseBase">
      <defName>TNA_crystalblood</defName>
      <label>crystal blood</label>
      <description>This immemorial and unique type of blood is born-given, which gives the infectors unreachable power.</description>
      <tendable>false</tendable>
      <isBad>false</isBad>
      <stages>
        <li>
          <capMods>
           <li>
            <capacity>BloodFiltration</capacity>
            <offset>1.00</offset>
          </li>
          <li>
            <capacity>BloodPumping</capacity>
            <offset>1.00</offset>
          </li>
          <li>
            <capacity>Breathing</capacity>
            <offset>0.50</offset>
          </li>
          <li>
            <capacity>Consciousness</capacity>
            <offset>0.50</offset>
          </li>
          <li>
            <capacity>Manipulation</capacity>
            <offset>1.00</offset>
          </li>
          <li>
            <capacity>Metabolism</capacity>
            <offset>1.00</offset>
          </li>
          </capMods>
        </li>
      </stages>
    </HediffDef>

    <!--凌之血-->
    <HediffDef ParentName="TNA_DiseaseBase">
      <defName>TNA_Seperatesblood</defName>
      <label>Linux's blood</label>
      <description>Owned by Linux Seperate,nobody else does, powerful and undefeatable.</description>
      <tendable>false</tendable>
      <isBad>false</isBad>
      <stages>
        <li>
          <capMods>
           <li>
            <capacity>BloodFiltration</capacity>
            <offset>1.00</offset>
          </li>
          <li>
            <capacity>BloodPumping</capacity>
            <offset>1.00</offset>
          </li>
          <li>
            <capacity>Breathing</capacity>
            <offset>1.00</offset>
          </li>
          <li>
            <capacity>Consciousness</capacity>
            <offset>1.00</offset>
          </li>
          <li>
            <capacity>Manipulation</capacity>
            <offset>1.00</offset>
          </li>
          <li>
            <capacity>Metabolism</capacity>
            <offset>1.00</offset>
          </li>
          <li>
            <capacity>Sight</capacity>
            <offset>1.00</offset>
          </li>
          <li>
            <capacity>Hearing</capacity>
            <offset>1.00</offset>
          </li>
          <li>
            <capacity>Moving</capacity>
            <offset>0.20</offset>
          </li>
          </capMods>
        </li>
      </stages>
    </HediffDef>

    <!--烈焰之血-->
    <HediffDef ParentName="TNA_DiseaseBase">
      <defName>TNA_campfiresblood</defName>
      <label>Campfire's blood</label>
      <description>Owned by Flux Campfire, giving her the super intellgence.</description>
      <tendable>false</tendable>
      <isBad>false</isBad>
      <stages>
        <li>
          <hungerRateFactor>2.00</hungerRateFactor>
          <restFallFactor>1.20</restFallFactor>
          <capMods>
           <li>
            <capacity>BloodFiltration</capacity>
            <offset>2.00</offset>
          </li>
          <li>
            <capacity>BloodPumping</capacity>
            <offset>2.00</offset>
          </li>
          <li>
            <capacity>Breathing</capacity>
            <offset>2.00</offset>
          </li>
          <li>
            <capacity>Consciousness</capacity>
            <offset>2.00</offset>
          </li>
          <li>
            <capacity>Metabolism</capacity>
            <offset>2.00</offset>
          </li>
          <li>
            <capacity>Eating</capacity>
            <offset>2.00</offset>
          </li>
          </capMods>
        </li>
      </stages>
    </HediffDef>

</Defs>